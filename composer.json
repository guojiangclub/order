{
  "name": "ibrand/order",
  "type": "library",
  "description": "iBrand order component",
  "keywords": [
    "iBrand",
    "order",
    "component"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "shjchen",
      "email": "ibrand.shjchen@foxmail.com"
    }
  ],
  "require": {
    "php": ">=7.0",
    "prettus/l5-repository": "^2.1"
  },
  "require-dev": {
    "mockery/mockery": "~1.0",
    "phpunit/phpunit": "~6.0",
    "orchestra/testbench": "~3.5",
    "orchestra/database": "~3.5"
  },
  "autoload": {
    "files": [
      "src/helpers.php"
    ],
    "psr-4": {
      "iBrand\\Component\\Order\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "iBrand\\Component\\Order\\Test\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "iBrand\\Component\\Order\\Providers\\OrderServiceProvider"
      ]
    }
  },
  "config": {
    "preferred-install": "dist"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
